{
  "Template": {
    "TemplateName": "TransactionSummaryTemplate",
    "SubjectPart": "Transactions Summary",
    "TextPart": "Here is the summary of your transactions.",
    "HtmlPart": "<html>\n<head></head>\n<body style=\"font-family: Arial, sans-serif;\">\n<header style=\"text-align: center; margin-bottom: 20px;\">\n    <img src=\"https://prueba-stori-bucket.s3.amazonaws.com/logo.svg\" alt=\"Company Logo\" style=\"max-width: 200px; height: auto;\">\n</header>\n<h2 style=\"text-align: center;\">Transaction Summary</h2>\n<table border=\"1\" cellpadding=\"10\" cellspacing=\"0\" style=\"border-collapse: collapse; width: 100%;\">\n<thead>\n<tr>\n<th style=\"text-align: left;\">Month</th>\n<th style=\"text-align: right;\">Value</th>\n</tr>\n</thead>\n<tbody>\n{{#transactions}}\n<tr>\n<td style=\"text-align: left;\">{{month}}</td>\n<td style=\"text-align: right;\">{{count}}</td>\n</tr>\n{{/transactions}}\n</tbody>\n</table>\n<br />\n<h3>Summary</h3>\n<p><strong>Total Debits:</strong> {{total_debits}}</p>\n<p><strong>Total Credits:</strong> {{total_credits}}</p>\n<p><strong>Credits Average:</strong> {{credit_avg}}</p>\n<p><strong>Debits Average:</strong> {{debit_avg}}</p>\n<p><strong>Total Balance:</strong> {{total_balance}}</p>\n</body>\n</html>"
  }
}
